{
  "name": "neutrino-ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build --stream",
    "build:watch": "lerna run build:watch --stream",
    "bootstrap": "lerna exec yarn",
    "linker": "lerna link",
    "test": "lerna run test --stream",
    "publish": "lerna publish",
    "dev": "yarn build && lerna exec yarn dev --scope storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/painted-pixels/neutrino-ui.git"
  },
  "dependencies": {
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@yarnpkg/pnpify": "^2.4.0",
    "concurrently": "^5.3.0",
    "lerna": "^3.22.1"
  }
}
