{
  "name": "neutrino-ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build --stream",
    "build:watch": "lerna run build:watch --stream",
    "bootstrap": "lerna bootstrap",
    "linker": "lerna link",
    "test": "jest --config jest.config.js --cache=false",
    "publish": "lerna publish",
    "lab": "yarn build && lerna exec yarn start --scope lab",
    "install-react-16": "npm i --no-save react@16 react-dom@16 react-test-renderer@16 enzyme-adapter-react-16"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/painted-pixels/neutrino-ui.git"
  },
  "peerDependencies": {
    "react": "16.x.x || 17.x.x",
    "react-dom": "16.x.x || 17.x.x"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/react": "^17.0.9",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "@yarnpkg/pnpify": "^2.4.0",
    "concurrently": "^6.2.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "jest": "^27.0.3",
    "lerna": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  }
}
